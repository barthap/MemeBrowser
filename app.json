{
  "expo": {
    "name": "Meme Browser",
    "slug": "meme-browser",
    "version": "0.1.0",
    "description": "Image gallery app with search-by-text feature.",
    "githubUrl": "https://github.com/barthap/MemeBrowser",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "experiments": {
      "redesignedLogBox": true
    }
  }
}
